CODE:
import random
a=True
while(a):
    temperature=random.randint(10,300)
    humidity=random.randint(10,300)
    if(temperature>100 and humidity>150):
        print("Alert!!!! High Temperature and humidity is Detected:",temperature,humidity,"%","Alarm is ON")
        a=False
    else:
        print("Normal Temperature:", temperature, "Normal Humidity:", humidity, "%", "Alarm is OFF")
        a=False
       
